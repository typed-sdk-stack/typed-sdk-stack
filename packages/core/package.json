{
    "name": "@typed-sdk-stack/core",
    "version": "0.0.0",
    "description": "Shared RapidAPI client primitives, retries, caching, and base SDK utilities.",
    "private": false,
    "license": "MIT",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "build": "tsup --config tsup.config.ts && tsc --project tsconfig.json --emitDeclarationOnly --outDir dist",
        "test": "bun test",
        "lint": "biome check .",
        "lint:fix": "biome check --write .",
        "check-types": "tsc --noEmit",
        "validate": "echo \"Validated\""
    },
    "dependencies": {
        "axios": "^1.13.2",
        "zod": "^4.3.4"
    },
    "devDependencies": {
        "@typed-sdk-stack/shared-config": "*",
        "axios-mock-adapter": "^2.1.0"
    },
    "peerDependencies": {
        "@types/bun": "^1.3",
        "typescript": "^5"
    }
}
